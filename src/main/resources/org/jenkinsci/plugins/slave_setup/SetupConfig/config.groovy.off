package org.jenkinsci.plugins.slave_setup.SetupConfig

def f = namespace(lib.FormTagLib)

f.section(title: _("Slave setup scripts")) {
    f.entry(title: _("Setup setup scripts")) {
        f.repeatable(var: "instance", field: instance.setupConfigItems) {
            System.out.println("<table width=\"100%\">")
            f.block("<b>(ATTENTION, slaves should be of one kind of System " +
                    "as there is only one script for all!)</b>")
            System.out.println("</table>")
        }
    }
}

